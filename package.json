{
  "name": "legado-fish-vscode",
  "displayName": "legado阅读摸鱼",
  "description": "阅读app vscode摸鱼",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "legado-fish-vscode.helloWorld",
        "title": "Hello World"
      }
